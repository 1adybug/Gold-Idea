generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Question {
  questionId Int    @id @default(autoincrement())
  content    String
  goal       String
  publisher  User?
}

model User {
  userId   Int      @id @default(autoincrement())
  avator   String?
  userName String
  policeNo String
  phone    String
  question Question @relation(fields: [userId], references: [questionId])
}
